{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "dist",
    "rootDir": "src",
    "noImplicitAny": false,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": [
    "dist",
    "__tests___modules",
    "coverage",
    ".eslintrc.cjs",
  ]
}