  "devDependencies": {
    "@graphql-typed-document-node/core": "^3.1.1",
    "@types/node": "^18.6.1",
    "@types/tap": "^15.0.7",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.11",
    "mercurius-integration-testing": "^5.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
